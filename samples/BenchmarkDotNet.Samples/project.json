{
  "title": "BenchmarkDotNet.Samples",
  "buildOptions": {
    "emitEntryPoint": true,
    "allowUnsafe": true
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true,
        "keyFile": "../../src/strongNameKey.snk",
        "strongName": true
      }
    }
  },
  "frameworks": {
    "net45": {
      "buildOptions": {
        "define": [ "CLASSIC" ]
      },
      "frameworkAssemblies": {
        "System.Reflection": "4.0.0.0"
      },
      "dependencies": {
        "BenchmarkDotNet.Diagnostics.Windows": {
          "target": "project"
        }
      }
    },
    "netcoreapp1.0": {
      "buildOptions": {
        "define": [ "CORE" ]
      },
      "dependencies": {
        "Microsoft.NETCore.App": "1.1.0",
        "System.ComponentModel.EventBasedAsync": "4.0.11"
      }
    }
  },
  "dependencies": {
    "BenchmarkDotNet": {
      "target": "project"
    },
    "System.Numerics.Vectors": "4.3.0"
  },

    "runtimes": {
    "win7-x64": {},
    "win8-x64": {},
    "win10-x64": {},
    "ubuntu.14.04-x64": {} 
  }
}